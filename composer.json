{
  "name": "plaisio/form-drop-down",
  "description": "PhpPlaisio: Dropdown Controls",
  "keywords": [
    "PhpPlaisio",
    "Form",
    "Dropdown"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "plaisio-ts/helper-cast": "^2.0.0",
    "plaisio-ts/kernel": "^2.1.0",
    "plaisio/form": "^0.55.2",
    "plaisio/helper-html": "^4.1.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/console-type-script": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Form\\Control\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
